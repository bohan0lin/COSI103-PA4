<h1><%= title %></h1>
    <p>Here is your list of transactions.</p >
    <a href="/transaction">Back to transaction table</a >
    <div class="container">
      <table class="table table-striped">
        <thead>
          <tr>
            <th scope="col" class="col-sm border">category</th>
            <th scope="col" class="col-sm border">amount</th>
          </tr>
        </thead>
        <tbody>
          <% results.map(item => { %>
            <tr>
                <td class="col-sm border p-around">
                    <%= item._id %>
                </td>
                <td class="col-sm border p-around"><%= item.total %></td>
            </tr>
          <% }) %>
        </tbody>
      </table>
    </div>